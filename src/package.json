{
  "name": "@dgwnu/fuseki-service-dev",
  "version": "0.0.1",
  "description": "Fuseki NPM CLI services development in TypeScript",
  "cli": "ts-node -P ts-node-config.json bin/fuseki-service-cli.ts",
  "scripts": {
    "build": "npm run build:lib && npm run build:cli",
    "build:cli": "tsc -p ts-cli-config.json",
    "build:lib": "rollup -c",
    "pm2:list": "pm2 l",
    "cli": "$npm_package_cli",
    "cli:run": "$npm_package_cli run",
    "cli:start": "$npm_package_cli start && pm2 l",
    "cli:restart": "$npm_package_cli restart && pm2 l",
    "cli:stop": "$npm_package_cli stop && pm2 l"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dgwnu/fuseki-service.git"
  },
  "keywords": [
    "fuseki",
    "cli",
    "development",
    "typescript"
  ],
  "author": "Onno Haldar <onno@haldar.nl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dgwnu/fuseki-service/issues"
  },
  "homepage": "https://github.com/dgwnu/fuseki-service#readme",
  "devDependencies": {
    "@types/node": "^14.14.22",
    "rollup": "^2.38.0",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "pm2": "^4.5.1",
    "rxjs": "^6.6.3"
  }
}
